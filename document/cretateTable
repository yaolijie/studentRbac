--10

drop table t_role;
drop table t_person;
drop table t_organ;
drop table t_role_connect_person;
drop table t_organ;
drop table t_organ_connect_person;

create table t_role(
  id varchar(64) primary key not null,
  rolename varchar(32),
  fullname varchar(64),
  code varchar(12),
  stardate date,
  enddate date,
  remarks varchar(128),
  createname varchar(32),
  createtime date,
  updatename varchar(32),
  updatetime date

);
create table t_person(
  id varchar(64) primary key not null,
  personname varchar(64),
  personid varchar(18),
  sex varchar(2),
  age int,
  fullname varchar(44),
  native varchar(10),
  emaill varchar(20),
  passworld varchar(12),
  birth varchar(12),
  positionId varchar(64),--出生地
  remarks varchar(256),
  createname varchar(32),
  createtime date,
  updatename varchar(32),
  updatetime date

);
create table t_role_connect_person (
  roleid varchar(64),
  person varchar(64),

);
create table t_organ (
  id varchar(64) primary key not null,
  organname varchar(128),
  type varchar(10),--机构是属于机构，还是部门
  code varchar(10),
  parentid varchar(64),
  address varchar(526),
  phone varchar(11),
  remark varchar(526),
  createname varchar(32),
  createtime date ,
  updatename varchar(32),
  updatetime date
);
create table t_organ_connect_person (
  organid varchar(64) primary key not null,
  personId varchar(64)
);

create table t_power(
    id varchaer(64) primary key not null,
    type varchar(20),
    createtime date;
);
create table t_operation_power{
    id varchar(64) primary key not null,
    name varchar(32),
    fullname varchar (32),
    code varchar(20),
    intercepUrl varchar(256),
    parentid varchar(64),
    createTime date;
};
create table t_menu_power(
    id varchar(64) primary key not null,
    name varchar(64),
    fullname varchar (32),
    code varchar (64),
    url varchar (128),
    parentId varchar (64),
    createtime date
) ;
create table t_menu_con_power(
    powerid varchar (64),
    menuid varchar (64);
);